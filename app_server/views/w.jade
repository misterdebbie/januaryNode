extends layout

include _includes/sharedHTMLfunctions


block content
  #banner.page-header
    .row
      .col-lg-6
        h1= pageHeader.title
          small &nbsp;#{pageHeader.strapline}
  .row
    .col-xs-12.col-sm-8
      .row.list-group
        each wish in wishlist
          .col-xs-12.list-group-item
            h4
              a(href="/wish")= wish.itemname
              small &nbsp;
                +outputRating(wish.rating)
            p.price= wish.itemprice

